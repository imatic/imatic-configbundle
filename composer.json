{
    "name": "imatic/config-bundle",
    "type": "symfony-bundle",
    "description": "Imatic Config Bundle",
    "keywords": [],
    "homepage": "https://www.imatic.cz",
    "license": "proprietary",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "https://www.imatic.cz"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.7",
        "symfony/form": "4.4|^5.3",
        "symfony/framework-bundle": "^4.4|^5.3",
        "symfony/translation": "^4.4|^5.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.2",
        "imatic/testing": "^6.0",
        "symfony/browser-kit": "^4.4|^5.3",
        "symfony/phpunit-bridge": "^4.4|^5.3",
        "symfony/security-bundle": "^4.4|^5.3",
        "symfony/twig-bundle": "^4.4|^5.3",
        "symfony/validator": "^4.4|^5.3",
        "symfony/yaml": "^4.4|^5.3"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\ConfigBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
